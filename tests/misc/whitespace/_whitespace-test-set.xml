      <test>
         <stylesheet file="whitespace-0102.xsl"/>
         <stylesheet file="import1.xsl" role="secondary"/>
      </test>
      <result>
         <assert-xml><![CDATA[<out><b>primary stylesheet</b><a> imported stylesheet </a></out>]]></assert-xml>
      </result>
   </test-case>

