<document>
<data>Saxon is great</data> 
</document>]]></content>
      </source>
   </environment>
   <environment name="eval012">
      <source role=".">
         <content><![CDATA[ 
<document xmlns="http://saxon097.uri/">
<path>following-sibling::data</path>
<data>Saxon is great</data> 
</document>]]></content>
