      </test>
      <result>
         <assert>deep-equal(/out/asc/item/string(), 
            ("document", "elements", "mechanism", "must", "namespaces", "prefix", "processors", "recognize", 
            "recognized", "specified", "to", "uri", "use", "xml", "xslt", "xsltdefined"))</assert>
      </result>
