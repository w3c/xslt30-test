      <result>
         <assert-xml><![CDATA[<result><out lang="en">B,C,K,T,U,V,X</out><out lang="de">B,C,K,T,U,V,X</out><out lang="sv">B,C,K,T,U,V,X</out></result>]]></assert-xml>
