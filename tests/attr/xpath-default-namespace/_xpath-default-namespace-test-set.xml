      <created by="Michael Kay" on="2012-10-30"/>
      <environment ref="xpath-default-namespace-08"/>
      <dependencies>
         <spec value="XSLT20+"/>
      </dependencies>
      <test>
         <stylesheet file="xpath-default-namespace-0801.xsl"/>
      </test>
      <result>
         <error code="XTSE0090"/>
      </result>
   </test-case>

