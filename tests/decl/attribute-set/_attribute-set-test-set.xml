         <spec value="XSLT10+"/>
      </dependencies>
      <test>
         <stylesheet file="attribute-set-1510.xsl"/>
      </test>
      <result>
         <assert-xml><![CDATA[<out rabbithole="deep" follow="yellowbrickroad" Alice="intoxicated" wife="thumbelina" location="Wonderland"/>]]></assert-xml>
      </result>
   </test-case>

