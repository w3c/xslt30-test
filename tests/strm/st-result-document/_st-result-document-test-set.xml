<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.w3.org/2012/10/xslt-test-catalog"
          name="st-result-document">
   <description>Tests xsl:stream instruction introduced in XSLT 3.0 with xsl:result-document.</description>
   
   <dependencies>
      <spec value="XSLT30+"/>
      <feature value="streaming"/>
   </dependencies>
   
   

   
   <test-case name="st-result-document-001">
      <description>Streaming - calling xsl:result-document - motionless content</description>
      <created by="Michael Kay" on="2013-06-15"/>
      <test>
         <stylesheet file="st-result-document-001.xsl"/>
         <initial-template name="main"/>
      </test>
      <result>
         <assert-result-document uri="76543210.xml">
               <assert-xml><![CDATA[<root>account</root>]]></assert-xml>
         </assert-result-document>
      </result>
   </test-case>


</test-set>
