      <dependencies>
         <spec value="XSLT10+"/>
      </dependencies>
      <test>
         <stylesheet file="node-0801.xsl"/>
      </test>
      <result>
         <assert-xml><![CDATA[<root>
A
B-C
TextNode_between_F_and_G
Yahoo
SecondNode_after_H
</root>]]></assert-xml>
      </result>
   </test-case>

