   <test-case name="node-0101">
      <description>Test for text() node test. Should say "test"</description>
      <created by="Michael Kay" on="2012-11-07"/>
      <environment ref="node-01"/>
      <dependencies>
         <spec value="XSLT10+"/>
      </dependencies>
      <test>
         <stylesheet file="node-0101.xsl"/>
      </test>
      <result>
         <assert-xml><![CDATA[<out>
  test
</out>]]></assert-xml>
      </result>
   </test-case>

